{
    "name": "express-typescript-example",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "build": "npx tsc",
        "start": "node dist/app.js",
        "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/app.js\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/config/AppDataSource.ts",
        "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create",
        "migration:generate": "npm run typeorm -- migration:generate",
        "migration:run": "npm run typeorm migration:run",
        "migration:revert": "npm run typeorm migration:revert",
        "schema:sync": "npm run typeorm schema:sync"
    },
